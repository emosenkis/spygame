<% provide(:title, "Edit Mission") %> 
<h1><%= @mission_template.title %></h1>

<div class="row">
  <div class="span6 offset3">
  <%= form_for(@mission_template) do |f| %>
      <%= render 'shared/error_messages' %>
      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :description %>
      <%= f.text_area :description %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>

  </div>
</div>